{
  "name": "mcp-openreyestr",
  "version": "1.0.0",
  "description": "MCP server for Ukrainian Unified State Register (OPENREYESTR)",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "dev:http": "tsx src/http-server.ts",
    "start": "node dist/index.js",
    "start:http": "node dist/http-server.js",
    "db:setup": "tsx src/migrations/migrate.ts",
    "migrate": "npm run build && node dist/migrations/migrate.js",
    "import:all": "tsx src/scripts/import-all-streaming.ts",
    "import:all:legacy": "tsx src/scripts/import-all.ts",
    "import:uo": "tsx src/scripts/import-uo.ts",
    "import:fop": "tsx src/scripts/import-fop.ts",
    "import:fsu": "tsx src/scripts/import-fsu.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts"
  },
  "keywords": [
    "mcp",
    "openreyestr",
    "ukraine",
    "registry",
    "legal-entities"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.6.1",
    "@types/sax": "^1.2.7",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fast-xml-parser": "^4.3.3",
    "pg": "^8.11.3",
    "redis": "^4.6.11",
    "sax": "^1.4.4",
    "stream-json": "^1.8.0",
    "unzipper": "^0.10.14",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@types/pg": "^8.10.9",
    "@types/unzipper": "^0.10.11",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
