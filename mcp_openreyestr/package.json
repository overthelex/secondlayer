{
  "name": "openreyestr-mcp",
  "version": "1.0.0",
  "description": "OpenReyestr MCP Server - NAIS registry data (debtors, legal entities, notaries) with MCP protocol",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "dev:http": "ts-node-dev --respawn --transpile-only src/http-server.ts",
    "start": "node dist/index.js",
    "start:http": "node dist/http-server.js",
    "db:create": "bash scripts/create-db.sh",
    "migrate": "npm run build && node dist/migrations/migrate.js",
    "db:setup": "npm run db:create && npm run migrate",
    "import:debtors": "npm run build && node dist/scripts/import-debtors.js",
    "import:entities": "npm run build && node dist/scripts/import-legal-entities.js",
    "test": "jest --no-cache --verbose",
    "test:watch": "jest --watch --no-cache --verbose",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [
    "mcp",
    "openreyestr",
    "nais",
    "debtors",
    "legal-entities",
    "ukraine"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@secondlayer/shared": "file:../packages/shared",
    "@types/express-session": "^1.18.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.17",
    "axios": "^1.6.5",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.18.2",
    "fast-xml-parser": "^5.3.4",
    "jsonwebtoken": "^9.0.3",
    "openai": "^4.20.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.11.3",
    "sax": "^1.4.4",
    "unzipper": "^0.12.3",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^9.39.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
