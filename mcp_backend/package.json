{
  "name": "secondlayer-mcp",
  "version": "1.0.0",
  "description": "Second Layer MCP for Zakononline - Semantic legal analysis layer",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "dev:http": "ts-node-dev --respawn --transpile-only src/http-server.ts",
    "dev:sse": "ts-node-dev --respawn --transpile-only src/sse-server.ts",
    "start": "node dist/index.js",
    "start:http": "node dist/http-server.js",
    "start:sse": "node dist/sse-server.js",
    "db:create": "bash scripts/create-db.sh",
    "migrate": "npm run build && node dist/migrations/migrate.js",
    "db:setup": "npm run db:create && npm run migrate",
    "process:docs": "npm run build && node dist/scripts/process-existing-documents.js",
    "process:docs:dev": "ts-node-dev --transpile-only scripts/process-existing-documents.ts",
    "migrate:legislation": "bash scripts/migrate-legislation-schema.sh",
    "test": "jest --no-cache --verbose",
    "test:watch": "jest --watch --no-cache --verbose",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [
    "mcp",
    "legal",
    "zakononline",
    "semantic-search"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@google-cloud/vision": "^5.3.4",
    "@modelcontextprotocol/sdk": "^1.25.3",
    "@qdrant/js-client-rest": "^1.9.0",
    "@secondlayer/shared": "file:../packages/shared",
    "@types/express-session": "^1.18.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.17",
    "axios": "^1.6.5",
    "canvas": "^3.2.1",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "diff": "^4.0.4",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.18.2",
    "jsonwebtoken": "^9.0.3",
    "mammoth": "^1.11.0",
    "openai": "^4.20.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pdf-parse": "^2.4.5",
    "pdfjs-dist": "^5.4.296",
    "pg": "^8.11.3",
    "playwright": "^1.58.0",
    "redis": "^4.6.12",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/diff": "^7.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
