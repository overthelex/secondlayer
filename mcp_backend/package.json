{
  "name": "secondlayer-mcp",
  "version": "1.0.0",
  "description": "Second Layer MCP for Zakononline - Semantic legal analysis layer",
  "main": "dist/index.js",
  "bin": {
    "secondlayer-mcp": "./bin/mcp-start.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "dev:http": "ts-node-dev --respawn --transpile-only src/http-server.ts",
    "dev:sse": "ts-node-dev --respawn --transpile-only src/sse-server.ts",
    "start": "node dist/index.js",
    "start:http": "node dist/http-server.js",
    "start:sse": "node dist/sse-server.js",
    "db:create": "bash scripts/create-db.sh",
    "migrate": "npm run build && node dist/migrations/migrate.js",
    "db:setup": "npm run db:create && npm run migrate",
    "process:docs": "npm run build && node dist/scripts/process-existing-documents.js",
    "process:docs:dev": "ts-node-dev --transpile-only scripts/process-existing-documents.ts",
    "migrate:legislation": "bash scripts/migrate-legislation-schema.sh",
    "seed:test-account": "npm run build && node dist/scripts/seed-test-account.js",
    "seed:test-account:dev": "ts-node-dev --transpile-only src/scripts/seed-test-account.ts",
    "test:email": "npm run build && node dist/scripts/test-email.js",
    "test:email:dev": "ts-node-dev --transpile-only src/scripts/test-email.ts",
    "docs": "xdg-open docs/index.html || open docs/index.html || start docs/index.html",
    "docs:api": "xdg-open docs/api-explorer.html || open docs/api-explorer.html || start docs/api-explorer.html",
    "test": "jest --no-cache --verbose",
    "test:watch": "jest --watch --no-cache --verbose",
    "lint": "eslint src --ext .ts",
    "create-api-keys": "ts-node bin/create-api-keys.ts"
  },
  "keywords": [
    "mcp",
    "legal",
    "zakononline",
    "semantic-search"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.74.0",
    "@google-cloud/vision": "^5.3.4",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@qdrant/js-client-rest": "^1.9.0",
    "@secondlayer/shared": "file:../packages/shared",
    "@types/bcryptjs": "^3.0.0",
    "@types/express-session": "^1.18.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.17",
    "axios": "^1.13.5",
    "bcryptjs": "^3.0.3",
    "bullmq": "^5.68.0",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "diff": "^4.0.4",
    "dotenv": "^17.2.4",
    "express": "^5.2.1",
    "express-session": "^1.18.2",
    "jsonwebtoken": "^9.0.3",
    "mammoth": "^1.11.0",
    "minio": "^8.0.6",
    "multer": "^2.0.2",
    "nodemailer": "^8.0.1",
    "openai": "^4.20.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pdf-parse": "^2.4.5",
    "pdfjs-dist": "^5.4.624",
    "pdfkit": "^0.17.2",
    "pg": "^8.11.3",
    "playwright": "^1.58.0",
    "redis": "^4.6.12",
    "stripe": "^20.3.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "word-extractor": "^1.0.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/diff": "^7.0.2",
    "@types/eventsource": "^3.0.0",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.2.0",
    "@types/nodemailer": "^6.4.22",
    "@types/pdfkit": "^0.17.4",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^9.39.2",
    "eventsource": "^4.1.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
