{
  "name": "secondlayer",
  "version": "1.0.0",
  "description": "Legal documents analysis platform with MCP server and admin panel",
  "private": true,
  "workspaces": [
    "mcp_backend",
    "mcp_rada",
    "mcp_openreyestr",
    "lexwebapp",
    "packages/shared"
  ],
  "scripts": {
    "backend": "cd mcp_backend && npm run dev:http",
    "frontend": "cd lexwebapp && npm run dev",
    "backend:build": "cd mcp_backend && npm run build",
    "frontend:build": "cd lexwebapp && npm run build",
    "install:all": "npm install",
    "batch-process": "tsx scripts/batch-process-documents.ts"
  },
  "keywords": [
    "mcp",
    "legal",
    "admin-panel",
    "zakononline"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.1",
    "@types/node": "^25.2.3",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.3",
    "commander": "^14.0.3",
    "dotenv": "^16.4.1",
    "ora": "^8.0.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tsx": "^4.7.0"
  },
  "overrides": {
    "postcss": "^8.4.49",
    "esbuild": ">=0.25.0",
"minio": {
      "fast-xml-parser": ">=5.3.4"
    }
  }
}
